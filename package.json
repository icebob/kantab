{
  "name": "kantab",
  "version": "0.1.0",
  "description": "Kanban board application with Moleculer microservices & Vue.js",
  "scripts": {
    "dev": "moleculer-runner --env --repl --hot backend/services",
    "start": "moleculer-runner --env",
    "now-start": "cross-env NODE_ENV=production moleculer-runner --env backend/services",
    "cli": "moleculer connect --ns kantab-prod nats://localhost:4222",
    "build": "vue-cli-service build",
    "now-build": "echo \"Skip build\"",
    "lint": "eslint backend && vue-cli-service lint",
    "deps": "npm-check -u",
    "postdeps": "npm test",
    "ci": "jest -c jest.backend.config.js --watch",
    "ci:e2e": "cross-env TEST_E2E=open MONGO_URI=mongodb://localhost/kantab-e2e moleculer-runner --env backend/services",
    "test": "npm run test:backend && npm run test:e2e",
    "test:backend": "jest -c jest.backend.config.js --coverage --no-cache",
    "test:frontend": "vue-cli-service test:unit",
    "test:e2e": "cross-env TEST_E2E=run MONGO_URI=mongodb://localhost/kantab-e2e moleculer-runner --env backend/services",
    "coverall": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "dc:up": "docker-compose up --build -d",
    "dc:down": "docker-compose down"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/icebob/kantab.git"
  },
  "author": "Icebob",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@babel/core": "7.2.0",
    "@babel/helper-module-imports": "7.0.0",
    "@babel/plugin-syntax-jsx": "7.2.0",
    "@vue/babel-preset-app": "3.2.0",
    "@vue/cli-plugin-babel": "3.2.0",
    "@vue/cli-plugin-e2e-cypress": "3.2.0",
    "@vue/cli-plugin-eslint": "3.2.1",
    "@vue/cli-plugin-pwa": "3.2.0",
    "@vue/cli-plugin-unit-jest": "3.2.0",
    "@vue/cli-service": "3.2.0",
    "@vue/eslint-config-airbnb": "4.0.0",
    "@vue/test-utils": "1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
